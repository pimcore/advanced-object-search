{
  "name": "pimcore/advanced-object-search",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "handcraftedinthealps/elasticsearch-dsl": "^8.0",
    "pimcore/elasticsearch-client": "^1.0",
    "pimcore/pimcore": "^10.6 || ^11.0",
    "symfony/config": "^5.2.0 || ^6.1",
    "symfony/console": "^5.2.0 || ^6.1",
    "symfony/dependency-injection": "^5.2.0 || ^6.1",
    "symfony/event-dispatcher": "^5.2.0 || ^6.1",
    "symfony/http-foundation": "^5.2.0 || ^6.1",
    "symfony/http-kernel": "^5.2.0 || ^6.1",
    "symfony/messenger": "^5.2.0 || ^6.1",
    "symfony/routing": "^5.2.0 || ^6.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9.11"
  },
  "autoload": {
    "psr-4": {
      "AdvancedObjectSearchBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "AdvancedObjectSearchBundle\\AdvancedObjectSearchBundle"
      ]
    }
  }
}
